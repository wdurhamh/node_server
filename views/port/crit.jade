          iframe(width="800px", onload="resizeIframe(this)", scrolling='no', frameborder='no', src='http://winstondhurst.me/spanish_portfolio/crit_reflection.html' )
        div(role="tabpanel" class="tab-pane" id="persuasive")
          iframe(width="800px", onload="resizeIframe(this)", scrolling='no', frameborder='no', src='http://winstondhurst.me/spanish_portfolio/persuasive.html' )
        div(role="tabpanel" class="tab-pane" id="research")
          iframe(width="800px", onload="resizeIframe(this)", scrolling='no', frameborder='no', src='http://winstondhurst.me/spanish_portfolio/researchpaper.html' )
