include port_header
  block content
    h3 Cultural Competence
    div
      <!-- Nav tabs -->
      ul(class="nav nav-tabs" role="tablist")
        li(role="presentation" class="active")
          a(href="#reflection" aria-controls="reflection" role="tab" data-toggle="tab") Reflection
        li(role="presentation")
          a(href="#persuasive" aria-controls="practice" role="tab" data-toggle="tab") Persuasive Paper
