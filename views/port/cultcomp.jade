include port_header
  block content
    h3 Cultural Competence
    div
      <!-- Nav tabs -->
      ul(class="nav nav-tabs" role="tablist")
        li(role="presentation" class="active")
          a(href="#reflection" aria-controls="reflection" role="tab" data-toggle="tab") Reflection
        li(role="presentation")
          a(href="#practice" aria-controls="practice" role="tab" data-toggle="tab") Cultural Practice
        li(role="presentation")
          a(href="#product" aria-controls="product" role="tab" data-toggle="tab") Cultural Product
      <!-- Tab panes -->
      div(class="tab-content")
        div(role="tabpanel" class="tab-pane active" id="reflection")
          iframe(width="800px", onload="resizeIframe(this)", scrolling='no', frameborder='no', src='http://winstondhurst.me/spanish_portfolio/cultural_reflection.html' )
        div(role="tabpanel" class="tab-pane" id="practice")
          iframe(width="800px", onload="resizeIframe(this)", scrolling='no', frameborder='no', src='http://winstondhurst.me/spanish_portfolio/fatherland.html' )
        div(role="tabpanel" class="tab-pane" id="product")
          iframe(width="800px", onload="resizeIframe(this)", scrolling='no', frameborder='no', src='http://winstondhurst.me/spanish_portfolio/cuartodeatras.html' )
